# Copy to .env and fill the values for production
# Provider API keys (optional - if omitted, local mocks will be used)
DEEPGRAM_API_KEY=
GROQ_API_KEY=
OPENAI_API_KEY=
TAVILY_API_KEY=
CARTESIA_API_KEY=

# Server settings
PORT=3000
# PREWARM_ON_STARTUP=true to run a short prewarm on server start (may incur API calls)
PREWARM_ON_STARTUP=false
# ENABLE_PERIODIC_PREWARM=true will run periodic prewarm tasks while process is alive
ENABLE_PERIODIC_PREWARM=false
PREWARM_INTERVAL_MS=900000

# WebSocket heartbeat ms
WS_HEARTBEAT_MS=30000
